            }
          }
        }
      }
    },
    "{urlPrefix}/healthz": {
      "get": {
        "tags": [ "Health Check" ],
        "summary": "Health Check (simple)",
        "description": "Returns a text/plain health status (pass, warn or fail)",
        "responses": {
          "200": {
            "description": "`string` (pass, warn or fail)",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                },
                "example": "pass"
              }
            }
          }
        }
      }
    },
