      "get": {
        "tags": [ "Health Check" ],
        "summary": "Health Check (IETF)",
        "description": "Returns an `IetfHealthCheck` document from the Health Check",
        "responses": {
          "200": {
            "description": "`IetfHealthCheck`",
            "content": {
              "application/health+json": {
                "schema": {
